<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>2.4.1</version>
        <id>datatables.net-scroller-bs5</id>
        <description>This is Scroller for DataTables with styling for [Bootstrap5](https://getbootstrap.com/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Scroller-Bootstrap5.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>Scroller  Virtual Scrolling  Bootstrap  Bootstrap5  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-scroller" version="2.0.7"/>
            <dependency id="datatables.net-bs5" version="1.12.1"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/scroller.bootstrap5.js" target="content\Scripts"/>
        <file src="js/scroller.bootstrap5.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/scroller.bootstrap5.min.js" target="content\Scripts"/>
        <file src="js/scroller.bootstrap5.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/scroller.bootstrap5.css" target="content\Content"/>
        <file src="css/scroller.bootstrap5.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/scroller.bootstrap5.min.css" target="content\Content"/>
        <file src="css/scroller.bootstrap5.min.css" target="contentFiles\any\any\wwwroot\css"/>
    </files>
</package>

